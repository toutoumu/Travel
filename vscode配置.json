{
    "editor.fontSize": 15,
    "editor.minimap.enabled": false,
    "editor.fontFamily": "'Courier New', Menlo, Monaco, monospace",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1.1,
    "terminal.integrated.fontWeightBold": "normal",
    "terminal.integrated.fontFamily": "'Courier New',Meslo LG M for Powerline",
    "java.errors.incompleteClasspath.severity": "ignore",
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "workbench.activityBar.visible": true,
    "window.zoomLevel": 1,
    "prettier.tabWidth": 4,
    "vetur.format.scriptInitialIndent": true,
    "vetur.format.styleInitialIndent": true,
    "vetur.format.defaultFormatter.js": "prettier-eslint",
    "prettier.singleQuote": true,
    "prettier.semi": false,
    "editor.formatOnType": true,
    "javascript.implicitProjectConfig.checkJs": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "java.configuration.checkProjectSettingsExclusions": false,
    "editor.wordWrapColumn": 120,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "eslint.enable": false,
    "javascript.validate.enable": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "px-to-rem.px-per-rem": 50,
    "files.associations": {
        "*.cjson": "jsonc",
        "*.wxss": "css",
        "*.wxs": "javascript"
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.list.openMode": "doubleClick",
    "emmet.includeLanguages": {
        "wxml": "html"
    },
    "minapp-vscode.disableAutoConfig": true,
    "gitlens.views.repositories.files.layout": "tree",
    "workbench.colorTheme": "One Dark Pro Bold",
    "workbench.colorCustomizations": {
        "tab.activeBackground": "#0f398d",
        "activityBar.background": "#282c34",
        "sideBar.background": "#2b2b2b",
        //"editor.background": "#282c34",
        "editor.background": "#191919",
    },
    "editor.tokenColorCustomizations": {
        "[One Dark Pro Bold]": {
            // "comments": "#77b766",
            // "keywords": "#cc7832",
            // "functions": "#ffc66d",
            // "strings": "#77b766",
            // "types": "#a8c022",
            // "variables": "#cc7832",
            // "numbers": "#6897bb",
            "textMateRules": [{
                "name": "文件中文字的样式,颜色",
                "scope": ["source.js", "source.json", "source.vue", "source.styl","source.css","source.html"],
                "settings": {
                    "fontStyle": "",
                    // "foreground": "#ff0000"
                },
            }, ]
        }
    }
}